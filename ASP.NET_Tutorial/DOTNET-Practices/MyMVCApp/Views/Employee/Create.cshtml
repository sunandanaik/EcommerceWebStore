@model MyMVCApp.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}

<h2>Employee Details</h2>
<table style="border:1px solid black;">
    <tbody>
        <tr>
            <th>Employee ID :</th>
            <td>@Model.EmpId</td>
        </tr>
        <tr>
            <th>Name :</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Gender :</th>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <th>City :</th>
            <td>@Model.City</td>
        </tr>
    </tbody>
</table>
@Html.ActionLink("Back to List","Index")
